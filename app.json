{
  "expo": {
    "name": "unplugged",
    "slug": "unplugged",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "AUDIO_RECORDING",
        "AUDIO_SETTINGS",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE"
      ],
      "package": "com.unplugged"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f56b7d15-21cf-4bfa-b6c0-9240358b2a0d"
      }
    }
  }
}